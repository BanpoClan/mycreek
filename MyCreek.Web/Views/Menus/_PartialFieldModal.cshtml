@using Abp.Extensions
@using MyCreek.Web.Models.Common.Modals


@Html.Partial("~/Views/Shared/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel("设置字段"))


<div class="modal-body">
   
    <form id="form">
        <div style="display:none;">
            <input type="text" class="form-control" name="id" value="@ViewBag.Id">
            <input type="text" class="form-control" name="MenuItemDefine_MenuGuid" value="@ViewBag.MenuItemDefine_MenuGuid">
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                
                <input type="text" class="form-control" name="ColName" value="@ViewBag.ColName">
                <label class="form-label">字段名称</label>
            </div>
        </div>
        <div class="form-group form-float">
            <div class="form-line">
                <input type="text" class="form-control" name="ColType" value="@ViewBag.ColType">
                <label class="form-label">字段类型</label>
            </div>
        </div>
        <div class="form-group form-float">
            <input type="checkbox" name="IsNull" id="IsNull"  value="@ViewBag.IsNull" class="filled-in chk-col-green" checked="">
            <label for="IsNull">是否可以为空</label>
        </div>

    </form>

</div>

@Html.Partial("~/Views/Shared/Modals/_ModalFooterWithSaveAndCancel.cshtml")